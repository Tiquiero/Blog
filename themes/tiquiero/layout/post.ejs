        <!-- Intro 头部显示 e -->
        <!-- Header 头部logo start -->
        <%- partial('_partial/headerlogo', {}, {cache: true}) %>
        <!-- Nav 导航条 start -->
        <%- partial('_partial/nav' , {}, {cache: true}) %>

        <div id="main" >
            <div class ="post_page_title_img" id="banner" style="height: 25rem;background-image: url(<%= page.thumbnail %>);background-position: center; background-repeat:no-repeat; background-size:cover;-moz-background-size:cover;overflow:hidden;" >
                <a href="#" style="padding: 4rem 4rem 2rem 4rem ;"><h2 ><%= page.title %></h2></a>
            </div>
            <!-- Post -->
            <div class="typo" style="padding: 3rem;">
                <%-page.content%>
            </div>

            <!-- Post Comments -->
            <%- partial('_partial/comment') %>

        </div>

        <a class="scrollTop toTop" id="toTop" href="javascript:;" style="display: block;"></a>
        <!-- Copyright 版权 start -->
        <%- partial('_partial/copyright', {}, {cache: true} ) %>
    </div>

